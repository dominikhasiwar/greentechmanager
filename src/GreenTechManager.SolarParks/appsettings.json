{
    "AuthorityEndpoint": "http://localhost:9001",    
    "BaseAddress": "http://localhost:9012",
    "ConnectionString": "",
    "MessageBus": {
        "HostName": "localhost",
        "Port": "5672"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Error",
                "System": "Error"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    }
}
